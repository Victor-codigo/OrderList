<?xml version="1.0" encoding="UTF-8"?>
<doctrine-mapping>
    <entity name="App\Orm\Entity\Profile" table="Profile">
        
        <id name="id" 
            type="string" 
            column-definition="CHAR(36) NOT NULL"/>

        <field name="image"
               type="string"
               length="256"
               nullable="true"/>
    </entity>
</doctrine-mapping>