<?xml version="1.0" encoding="UTF-8"?>
<doctrine-mapping>
    <entity name="User\Domain\Model\Profile" table="Profile">
        
        <id name="id" 
            type="IdentifierType" 
            column-definition="CHAR(36) NOT NULL"/>

        <field name="image"
               type="PathType"
               length="256"
               nullable="true"/>
    </entity>
</doctrine-mapping>